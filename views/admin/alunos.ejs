<div class="container">
  <%- include('./forms/form-alunos') %>
  <div class="buscar">
    <form class="form-buscar" action="/admin/alunos/1/buscar">
      <input class="input-buscar" type="text" name="nome">
      <button class="btn btn-light" type="submit">Buscar</button>
    </form>
    <button class="btn-new" id="btnNovoAluno" type="button" data-toggle="modal" data-target="#criarAluno" data-whatever="@mdo" data-toggle="tooltip" data-placement="right" title="Nova Matrícula">
      <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-plus-circle-fill" viewBox="0 0 16 16">
        <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM8.5 4.5a.5.5 0 0 0-1 0v3h-3a.5.5 0 0 0 0 1h3v3a.5.5 0 0 0 1 0v-3h3a.5.5 0 0 0 0-1h-3v-3z"/>
      </svg>
    </button>
  </div>
  <table class="table table-responsive">
    <thead>
      <tr>
        <th>Id</th>
        <th>Nome</th>
        <th>Sobrenome</th>
        <th>Turma</th>
        <th class="center">Ações</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td class="">1</td>
        <td class="">Ana</td>
        <td class="">Almeida</td>
        <td>1º Ano</td>
        <td class="turno">
          <a class="botao botao-detalhes" href="#">Detalhes</a>
        </td>
      </tr>
    </tbody>
  </table>
</div>

